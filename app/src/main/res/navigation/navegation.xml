<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegation"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.applicationlanguage.login.FirstFragment"
        android:label="Inici"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_firstFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_firstFragment_to_registreFragment"
            app:destination="@id/registreFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_firstFragment_to_idiomaFragment"
            app:destination="@id/idiomaFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/firstFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.applicationlanguage.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_idiomaFragment"
            app:destination="@id/idiomaFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/firstFragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_loginFragment_to_registreFragment"
            app:destination="@id/registreFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/registreFragment"
        android:name="com.example.applicationlanguage.login.RegistreFragment"
        android:label="fragment_registre"
        tools:layout="@layout/fragment_registre" >
        <action
            android:id="@+id/action_registreFragment_to_idiomaFragment"
            app:destination="@id/idiomaFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/firstFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/idiomaFragment"
        android:name="com.example.applicationlanguage.idioma.IdiomaFragment"
        android:label="fragment_idioma"
        tools:layout="@layout/fragment_idioma" >
        <action
            android:id="@+id/action_idiomaFragment_to_temaFragment"
            app:destination="@id/temaFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/temaFragment"
        android:name="com.example.applicationlanguage.idioma.TemaFragment"
        android:label="fragment_tema"
        tools:layout="@layout/fragment_tema" >
        <action
            android:id="@+id/action_temaFragment_to_paraulasOTestFragment"
            app:destination="@id/paraulasOTestFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_temaFragment_to_conocimientoFragment"
            app:destination="@id/conocimientoFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/paraulasOTestFragment"
        android:name="com.example.applicationlanguage.idioma.ParaulasOTestFragment"
        android:label="fragment_paraulas_o_test"
        tools:layout="@layout/fragment_paraulas_o_test" >
        <action
            android:id="@+id/action_paraulasOTestFragment_to_testFragment"
            app:destination="@id/testFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_paraulasOTestFragment_to_mostrarFragment"
            app:destination="@id/mostrarFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.applicationlanguage.test.TestFragment"
        android:label="fragment_test"
        tools:layout="@layout/fragment_test" >
        <action
            android:id="@+id/action_testFragment_to_finTestFragment"
            app:destination="@id/finTestFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/temaFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/finTestFragment"
        android:name="com.example.applicationlanguage.test.FinTestFragment"
        android:label="fragment_fin_test"
        tools:layout="@layout/fragment_fin_test" >
        <action
            android:id="@+id/action_finTestFragment_to_temaFragment"
            app:destination="@id/temaFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/temaFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/conocimientoFragment"
        android:name="com.example.applicationlanguage.idioma.ConocimientoFragment"
        android:label="fragment_conocimiento"
        tools:layout="@layout/fragment_conocimiento" >
        <action
            android:id="@+id/action_conocimientoFragment_to_paraulasOTestFragment"
            app:destination="@id/paraulasOTestFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/mostrarFragment"
        android:name="com.example.applicationlanguage.mostrar.MostrarFragment"
        android:label="fragment_mostrar"
        tools:layout="@layout/fragment_mostrar" >
        <action
            android:id="@+id/action_mostrarFragment_to_alfabetoMostrarFragment"
            app:destination="@id/alfabetoMostrarFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:popUpTo="@id/mostrarFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/alfabetoMostrarFragment"
        android:name="com.example.applicationlanguage.mostrar.AlfabetoMostrarFragment"
        android:label="fragment_alfabeto_mostrar"
        tools:layout="@layout/fragment_alfabeto_mostrar" />
</navigation>